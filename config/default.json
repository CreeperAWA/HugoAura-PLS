{
  "proxyProcArr": ["SeewoCore.exe"],
  "wsHost": "127.0.0.1",
  "wsPort": 22077,
  "regenCert": true,
  "certPath": "config/certs/cert.crt",
  "keyPath": "config/certs/cert.key",
  "ruleSettings": {
    "client": {
      "security": {
        "uploadFreezeInfo": {
          "enable": false,
          "rewriteMode": "allFreeze"
        }
      }
    },
    "server": {
      "security": {
        "dispatchBrowseAuditMode": {
          "enable": false,
          "mode": "disableAudit"
        },
        "dispatchBrowseAuditStrategy": {
          "enable": false,
          "strategyType": "blackList",
          "allowSites": ["seewo.com", "cvte.com", "upload.qiniup.com"],
          "forbiddenSites": []
        }
      }
    }
  }
}
